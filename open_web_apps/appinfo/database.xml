<?xml version="1.0" encoding="ISO-8859-1" ?>
<database>
	 <name>*dbname*</name>
	 <create>true</create>
	 <overwrite>false</overwrite>
	 <charset>latin1</charset>
	 <table>
		<name>*dbprefix*open_web_apps</name>
		<declaration>
			<field>
				<name>uid_owner</name>
				<type>text</type>
				<primary>true</primary>
				<notnull>true</notnull>
				<length>64</length>
			</field>
			<field>
				<name>app_id</name>
				<type>text</type>
				<primary>true</primary>
				<notnull>true</notnull>
				<length>100</length>
			</field>
			<field>
				<name>access_token</name>
				<type>text</type>
				<length>100</length>
			</field>
		</declaration>
	</table>
	 <table>
		<name>*dbprefix*remotestorage_access</name>
		<declaration>
			<field>
				<name>uid_owner</name>
				<type>text</type>
				<primary>true</primary>
				<notnull>true</notnull>
				<length>64</length>
			</field>
			<field>
				<name>access_token</name>
				<type>text</type>
				<primary>true</primary>
				<notnull>true</notnull>
				<length>100</length>
			</field>
			<field>
				<name>module</name>
				<type>text</type>
				<length>100</length>
			</field>
			<field>
				<name>level</name>
				<type>text</type>
				<length>10</length>
			</field>
		</declaration>
	</table>
</database>
